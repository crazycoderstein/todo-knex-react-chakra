{
  "name": "fullstack-challenge",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "deps": "(npm i) && (cd web && npm i) && (cd .. && cd api && npm i)",
    "api": "cd api && nodemon",
    "web": "cd web && npm start",
    "start": "concurrently --kill-others-on-fail \"npm run api\" \"npm run web\""
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^10.9.1"
  }
}
